{% extends 'base.html.twig' %}

{% block body %}

    <h1>Inscription sur le site</h1>

    {{form_start(form)}}
    {{form_row(form.nom)}}
    {{form_row(form.prenom)}}
    {{form_row(form.email)}}
    {{form_row(form.password)}}
    {{form_row(form.confirm_password)}}
    <button type="submit">Inscription</button>
    {{form_end(form)}}
{% endblock %}